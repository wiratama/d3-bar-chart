<h1>Listing barcarts</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @barcarts.each do |barcart| %>
      <tr>
        <td><%= link_to 'Show', barcart %></td>
        <td><%= link_to 'Edit', edit_barcart_path(barcart) %></td>
        <td><%= link_to 'Destroy', barcart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

